<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.huanzi.qch.springbootmybatis.mapper.UserInfoMapper">

    <resultMap id="UserResult" type="cn.huanzi.qch.springbootmybatis.domain.UserInfo">
        <id property="id" column="id" jdbcType="INTEGER"/>
        <id property="name" column="name"/>
        <id property="passWord" column="pass_word" jdbcType="VARCHAR"/>
    </resultMap>

    <select id="selectUserList" parameterType="userInfo" resultType="userInfo">
        select * from user_info
    </select>

    <select id="getUserById" parameterType="userInfo" resultType="userInfo">
        select * from user_info where id = #{id}
    </select>

</mapper>